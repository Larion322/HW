print('Введите целое число')
a=int(ввод())
если a в диапазоне (10,22) или a в диапазоне (31,40):
 print('Точка в множестве')
ещё:
 print('Точка вне множества')
    
print('Введите целое число')
a=int(ввод())
print('Введите целое число')
b=int(ввод())
если (a в диапазоне (-5,1) и b в диапазоне (-5,1)) или (a в диапазоне (3,8) и b в диапазоне (3,8)):
 печать ('Да')
ещё:
 print('Нет')
    
print('Введите значение пути')
a=значение с плавающей запятой(ввод())
print('Введите значение времени')
b=float(input())
c=a/b
если c<30:
 print('Медленно')
если c>30 и c<=60:
 print('Средне')
elif c>60:
 print('Быстро')

print('Введите трёхзначное число:')
x=int(ввод())
в то время как x<100 или x>999:
 если x<100 или x>999:
 print('Число должно быть трёхзначным!')
 print('Введите трёхзначное число:')
 x=int(ввод())
a=x//100
b=x//10%10
c=x%10
d=a+b+c-максимум (a, b, c)-минимум (a, b, c)
если d==max(a, b, c)-min(a,b, c):
 print('Число n "интересное"')
ещё:
 print('Число n не "интересное"')
 
print('Введите ник-нейм')
a=str(ввод())
если len(a)<3:
 print('Ник-нейм должен состоять минимум из 3 символов!')
elif len(a)>15:
 print('Ник-нейм должен состоять максимум из 15 символов!')
ещё:
 print('Ник-нейм принят')
    
print('Ввведите значение x:')
x=float(input())
print('Введите значение y')
y=значение с плавающей запятой(ввод())
в то время как x==0 или y==0:
 если x==0 или y==0:
 print('X и Y не должны равняться 0')
 print('Ввведите значение x:')
 x=значение с плавающей запятой(ввод())
 print('Введите значение y')
 y=значение с плавающей запятой(ввод())
если x>0 и y>0:
 print('Точка принадлежит первой четверти')
 print('Введите две строки')
 a=str(ввод())
 b=str(ввод())
 если len(a)>len(b):
 печать(b)
 ещё:
 вывести(a)
если x<0 и y>0:
 print('Точка принадлежит второй четверти')
 f=(2* abs(x)-y)/y**2
 печать(f)
если x<0 и y<0:
 print('Введите строку')
 a=str(ввод())
 print('Точка принадлежит третьей четверти')
 вывести('x=',x*len(a), 'y=',y*len(a))
ещё:
 print('Точка принадлежит четвёртой четверти')
 если abs(x)>abs (y):
 print('Координата x – наибольшая по модулю')
 elif abs (y)==abs (x):
 print('«Координаты равны по модулю')
 ещё:
 print('Координата y – наибольшая по модулю')
        
